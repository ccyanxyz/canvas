type SuccessUpdate = 
 record {
   accountBalance: nat;
   newHeight: nat;
 };
type Result = 
 variant {
   err: Error;
   ok: SuccessUpdate;
 };
type Image = vec vec text;
type Error = 
 variant {
   BadRequest;
   InsufficientCycles;
 };
service : {
  getHistory: () -> (vec Image) query;
  latest: () -> (Image) query;
  upload: (Image) -> (Result);
  wallet_receive: () -> (nat);
}
